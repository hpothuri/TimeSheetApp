<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="timeSheetTF">
    <default-activity>initTimeSheet</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <input-parameter-definition id="__2">
      <name>timeSheetId</name>
      <value>#{pageFlowScope.timeSheetId}</value>
    </input-parameter-definition>
    <managed-bean id="__1">
      <managed-bean-name>TimeSheetHoursBean</managed-bean-name>
      <managed-bean-class>view.beans.TimeSheetHoursBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <view id="viewTimeSheet">
      <page>/viewTimeSheet.jsff</page>
    </view>
    <method-call id="initTimeSheet">
      <method>#{TimeSheetHoursBean.initTimeSheet}</method>
      <outcome>
        <fixed-outcome>view</fixed-outcome>
      </outcome>
    </method-call>
    <method-call id="SetCurrentRowWithKeyValue">
      <method>#{bindings.setCurrentRowWithKeyValue.execute}</method>
      <outcome>
        <fixed-outcome>setCurrentRowWithKeyValue</fixed-outcome>
      </outcome>
    </method-call>
    <router id="createOrUpdate">
      <case id="__3">
        <expression>#{pageFlowScope.timeSheetId == null}</expression>
        <outcome>create</outcome>
      </case>
      <case id="__4">
        <expression>#{pageFlowScope.timeSheetId != null}</expression>
        <outcome>edit</outcome>
      </case>
      <default-outcome>create</default-outcome>
    </router>
    <method-call id="createTimeSheet">
      <method>#{requestScope.TimeSheetHoursBean.createTimeSheet}</method>
    </method-call>
    <control-flow-rule id="__6">
      <from-activity-id>initTimeSheet</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>view</from-outcome>
        <to-activity-id>viewTimeSheet</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__5">
      <from-activity-id>createOrUpdate</from-activity-id>
      <control-flow-case id="__9">
        <from-outcome>edit</from-outcome>
        <to-activity-id>SetCurrentRowWithKeyValue</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__8">
        <from-outcome>create</from-outcome>
        <to-activity-id>createTimeSheet</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__10">
      <from-activity-id>SetCurrentRowWithKeyValue</from-activity-id>
      <control-flow-case id="__11">
        <from-outcome>setCurrentRowWithKeyValue</from-outcome>
        <to-activity-id>initTimeSheet</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__12">
      <from-activity-id>createTimeSheet</from-activity-id>
      <control-flow-case id="__13">
        <from-outcome>creatTimeSheet</from-outcome>
        <to-activity-id>viewTimeSheet</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>

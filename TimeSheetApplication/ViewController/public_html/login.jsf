<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE html>
<f:view xmlns:f="http://java.sun.com/jsf/core" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
        xmlns:c="http://java.sun.com/jsp/jstl/core">
    <c:set var="viewcontrollerBundle" value="#{adfBundle['view.ViewControllerBundle']}"/>
    <af:document title="login.jsf" id="d1">
        <af:form id="f1">
            <af:panelGroupLayout id="pgl1" layout="vertical" halign="center">
                <af:spacer width="10" height="100" id="s1"/>
                <af:image source="/images/SkyBridge-Sign_Doc Logo.jpg" id="i1"/>
            </af:panelGroupLayout>
            <af:spacer width="10" height="30" id="s2"/>
            <af:panelGroupLayout id="pgl2" layout="vertical" halign="center"
                                 inlineStyle='background-image:url("images/loginBackground.png"); background-repeat:no-repeat; background-position:center;'>
                <af:panelFormLayout id="pfl1">
                    <af:inputText label="#{viewcontrollerBundle.Username}" id="it1" value="#{LoginBean.userName}"/>
                    <af:inputText label="#{viewcontrollerBundle.Password}" id="it2" secret="true"
                                  value="#{LoginBean.password}"/>
                    <f:facet name="footer">
                        <af:panelGroupLayout id="pgl3" layout="horizontal" halign="end" styleClass="AFStretchWidth">
                            <af:button text="#{viewcontrollerBundle.Login}" id="b1" action="#{LoginBean.doLogin}"/>
                        </af:panelGroupLayout>
                    </f:facet>
                </af:panelFormLayout>
            </af:panelGroupLayout>
        </af:form>
    </af:document>
    <!--oracle-jdev-comment:preferred-managed-bean-name:LoginBean-->
</f:view>